<body>
<!-- Begin Wrapper -->
<div id="wrapper">
  <div class="wrapper-top"></div>
  <div class="wrapper-mid">
    <!-- Begin Paper -->
    <div id="paper">
      <div class="paper-top"></div>
      <div id="paper-mid">
        <div class="entry">
          <!-- Begin Image -->
          <img class="portrait" src="images/style_patient/images/patient/patient1.png" alt="John Smith" />
          <!-- End Image -->
          <!-- Begin Personal Information -->
          <div class="self">
            <h1 class="name">Patient ID - <% if(user){ %><%=user.patient_id%><% } else { %>error<%}%><br />
              <span></span></h1>
            <ul>
              <li class="tel">Name - <% if(user){ %><%=user.firstname%> <%=user.lastname%><% } else { %>error<%}%></li>
              <li class="ad">Date - <% if(user){ %><%=user.date%><% } else { %>error<%}%></li>
              <li class="mail">Time - <% if(user){ %><%=user.time%><% } else { %>error<%}%></li>
              
              <li class="web">www.businessweb.com</li>
            </ul>
          </div>
          <!-- End Personal Information -->
          <!-- Begin Social -->
          <div class="social">
            <ul>
              <button onclick="myFunction()"><li><a id="myAnchor" class='north' href="http://www.web2pdfconvert.com/convert" title="Download .pdf"><img src="images/style_patient/images/icn-save.jpg" alt="Download the pdf version" /></a></li></button>
              <button><li><a href="http://localhost:3000"><img src="images/style_patient/images/icn-home.v01.ico"></a></li></button>
              
            </ul>
          </div>
          <!-- End Social -->
        </div>
        <!-- Begin 1st Row -->
        <div class="entry">
          <h2>Nature of Problem & Medication</h2>
          <p><% if(user){ %><%=user.problem%><% } else { %>error<%}%></p>
        </div>
        <!-- End 1st Row -->
        <!-- Begin 2nd Row -->
        <div class="entry">
          <h2>Injection Name</h2>
          <div class="content">
            <h3></h3>
            <p><% if(user){ %><%=user.injection_name%><% } else { %>error<%}%><br />
              <em></em></p>
          </div>
         
        </div>
        <!-- End 2nd Row -->
        <!-- Begin 3rd Row -->
        <div class="entry">
          <h2>Other Medication</h2>
          <div class="content">
            <h3>Injection</h3>
            <p><% if(user){ %><%=user.injection_yes%><% } else { %>error<%}%><br />
           
          </div>
          <div class="content">
            <h3>Dressing</h3>
            <p><% if(user){ %><%=user.dressing_yes%><% } else { %>error<%}%><br />
              
          </div>
        </div>
        <!-- End 3rd Row -->
        <!-- Begin 4th Row -->
        
         
        <!-- End 4th Row -->
         <!-- Begin 5th Row -->
