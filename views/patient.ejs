<script>
var url      = window.location.href;
function myFunction() {
    document.getElementById("myAnchor").href = "http://api.html2pdfrocket.com/pdf?value="+url+"&apikey=abe1e5fc-84d8-4bc2-b76f-40c6144d7cd1";
    

}
</script>
<script type="text/javascript" src="images/style_patient/js/jquery-1.4.2.min.js"></script>
<script type="text/javascript" src="images/style_patient/js/jquery.tipsy.js"></script>
<script type="text/javascript" src="images/style_patient/js/cufon.yui.js"></script>
